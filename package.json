    "proj4": "^2.19.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.25",
    "typeorm-transactional": "^0.5.0",
    "workerpool": "^9.3.3"
